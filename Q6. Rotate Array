https://leetcode.com/problems/rotate-array/?envType=study-plan-v2&envId=top-interview-150

Solution:

class Solution {
    public void rotate(int[] nums, int k) {
        int n = nums.length;
        if(n<=1){
            return;
        }
        k = k%n;
        int[] arr1 = new int[n-k];
        int[] arr2 = new int[k];
        for(int i=0; i<(n-k); i++){
            arr1[i] = nums[i]; 
        }
        int j = 0;
        for(int i=(n-k); i<n; i++){
            arr2[j++] = nums[i];
        }
        reverse(arr1);
        reverse(arr2);
        for(int i=0; i<n-k; i++){
            nums[i] = arr1[i];
        }
        j=0;
        for(int i=n-k; i<n; i++){
            nums[i] = arr2[j++];
        }
        reverse(nums);
    }
    public void reverse(int[] arr){
        int n = arr.length;
        for(int i=0; i<n/2; i++){
            int temp = arr[i];
            arr[i] = arr[n-i-1];
            arr[n-i-1] = temp; 
        }
    }
}
